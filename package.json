{
  "name": "vue-simple-gallery-slider",
  "description": "A dummy simple gallery slider for vue",
  "version": "0.1.18",
  "private": false,
  "author": {
    "name": "Marshall Henhawke",
    "email": "marsh.hawe@gmail.com"
  },
  "repository": "https://github.com/marshallhenhawke/vue-simple-gallery",
  "bugs": "https://github.com/marshallhenhawke/vue-simple-gallery/issues",
  "keywords": [
    "vue",
    "simple",
    "gallery",
    "slider"
  ],
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "publish-package": "npm run build-library && npm version patch -m \"build: release %s\" && npm publish --access public",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-library": "vue-cli-service build --target lib --name vue-simple-gallery-slider ./src/install.js",
    "lint": "vue-cli-service lint"
  },
  "main": "./dist/vue-simple-gallery-slider.common.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "url-loader": "^2.3.0",
    "vue": "^2.6.11",
    "vue-svg-loader": "^0.16.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
